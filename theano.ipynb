   "source": [
    "import theano.tensor as T\n",
    "from numpy import array\n",
    "x = T.dmatrix('x')\n",
    "s = 1 / (1 + T.exp(-x))\n",
    "logistic = theano.function([x], s)\n"
   ]
  },
